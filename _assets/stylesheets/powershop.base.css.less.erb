/* 
 * -------------------------------------------------------- 
 * Side nav
 * -------------------------------------------------------- 
 */
#jumper {
	position: fixed;
	top: 50%;
	margin-top: -100px;
	right: 0;
	z-index: 80;

	li {
		position: relative;
		margin-bottom: 10px;
		height: 10px;
		padding-right: 25px;

		a {
			display: block;
			height: 8px;
			width: 8px;
			background: #fff;
			background: rgba(221, 221, 221, 0.4);
			border-radius: 8px;
			text-indent: -9000px;

			.drop-shadow(0, 0, 5px, 0, 0.3);

			&:hover,
			&:focus {
				background: rgba(221, 221, 221, 0.8);
			}
		}

		&.active {
			a {
				background: rgba(255, 255, 255, 1);
			}

			&:after {
				content: " ";
				display: block;
				position: absolute;
				right: 0;
				height: 0;
				width: 0;
				top: -2px;
				border-top: 6px solid transparent;
				border-bottom: 6px solid transparent;
				border-right: 6px solid #fff;
			}
		}
	}
}

.touch #jumper {
	display: none;
}

.parallax {	
	top: 0;
	width: 100%;
	left: 0;

	&.last {}

	.parallax_content_layer {
		position: relative;
		overflow: hidden;
	}

	.parallax_background_layer {
		width: 100%;
		overflow: hidden;
	}
}

