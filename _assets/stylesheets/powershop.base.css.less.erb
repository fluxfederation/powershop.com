#loading {
	background: @blueGray url(<%= asset_path('textures/blue_gray.png') %>) repeat;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	right: 0;
	bottom: 0;
	height: 100%;
	z-index: 500;

	.loading_icon {
		display: block;
		position: fixed;
		-webkit-animation: rotating 2s linear infinite;
		-moz-animation: rotating 2s linear infinite;
		animation: rotating 2s linear infinite;
		top: 50%;
		left: 50%;
		width: 306px;
		height: 306px;
		z-index: 501;
		margin: -153px 0 0 -153px;
		.transition(opacity 0.5s ease-in-out);
		background: url(<%= asset_path('loading_element.png') %>) no-repeat;
	}

	&:after {
		content: " ";
		display: block;
		position: fixed;
		height: 248px;
		width: 251px;
		top: 50%;
		left: 50%;
		margin: -124px 0 0 -125px;
		z-index: 502;
		background: url(<%= asset_path('loading_face.png') %>) center center no-repeat;
	}


	&.done:after {
		opacity: 0;
	}

	.loading_eye {
		background: #2d3439 url(<%= asset_path('loading_face_wink_eye.png') %>) bottom right no-repeat;
		position: fixed;
		display: block;
		height: 90px;
		width: 90px;
		z-index: 503;
		top: 50%;
		left: 50%;
		margin: -85px 0 0 -3px;

		.transform-origin(70%, 70%);
		.transition(all 1s ease-in 0.8s);
		.rotate(0);
	}

	&.wink .loading_eye {
		.rotate(-15deg);
	}
}

