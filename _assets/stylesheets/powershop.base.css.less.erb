/* 
 * -------------------------------------------------------- 
 * Core styles 
 *
 * Vertical Rhythm: 5px 
 * -------------------------------------------------------- 
 */

.down_arrow(@color) {
	content: " ";
	position: absolute;
	height: 0;
	width: 0;
	bottom: -14px;
	z-index: 2;
	margin-left: -14px;
	border-left: 14px solid transparent;
	border-right: 14px solid transparent;
	border-top: 14px solid @color;
}

.up_arrow(@color) {
	content: " ";
	position: absolute;
	height: 0;
	width: 0;
	left: 50%;
	top: -14px;
	z-index: 2;
	margin-left: -14px;
	border-left: 14px solid transparent;
	border-right: 14px solid transparent;
	border-bottom: 14px solid @color;
}

.hr(@color) {
	position: relative;
	padding-bottom: 29px;
	margin-bottom: 35px;

	&:after {
		content: " ";
		width: 75px;
		height: 1px;
		left: 0;
		position: absolute;
		bottom: 0;
		opacity: 0.2;
		background: @color;
	}
}

html {
	overflow-y: auto; /* force scroll bar */
}

